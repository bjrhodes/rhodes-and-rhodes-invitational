---
import CollectionLayout from "@components/fixtures/CollectionLayout.astro";
import { getEntries } from "@lib/contentParser";
import { sortByDate } from "@lib/sortFunctions";
import type { FixturesEntry } from "@/types";
import { getIndex } from "@lib/contentParser";

const entryIndex = (await getIndex("fixtures")) as FixturesEntry;
const entries = (await getEntries("fixtures", sortByDate)) as FixturesEntry[];
---

<CollectionLayout entryIndex={entryIndex} entries={entries} />
